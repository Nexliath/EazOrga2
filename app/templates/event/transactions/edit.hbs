{{#if (is-equal form.formType "transfer")}}
  <TransferForm @transfer={{form}} @onDelete={{route-action "delete"}} @title="View Transfer" />
{{else}}
  <TransactionForm @transaction={{form}} @users={{users}} @onDelete={{route-action "delete"}} @onModelUpdated={{route-action "modelUpdated"}} @title="Edit Transaction" />
{{/if}}
